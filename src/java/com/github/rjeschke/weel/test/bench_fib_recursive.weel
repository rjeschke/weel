func fib_weel(n)
  if n < 2 then
    return n
  else
    return fib_weel(n - 2) + fib_weel(n - 1)
  end
end

sub main(args)
    local times = toNum(args[0]) || 5;
    local n = toNum(args[1]) || 30;

    for i = 1, times do
        t = clock();
        fib_weel(n);
        t = clock() - t;
        println(i .. ": " .. t);
    end
end
